local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("CHUB+ (ADDED blade bell soon)", "Synapse")
local Tab = Window:NewTab("PSX")
local Section = Tab:NewSection("PSX TRADE SCAM")
Section:NewButton("on", "ButtonInfo", function()
    print("Clicked")
end)
Section:NewButton("off", "ButtonInfo", function()
    print("Clicked")
end)
Section:NewTextBox("report bugs here", "TextboxInfo", function(txt)
	print(txt)
end)
